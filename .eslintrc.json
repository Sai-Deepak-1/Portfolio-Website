{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "warn",
    "no-undef": "warn",
    "no-unreachable": "warn",
    "no-constant-condition": "warn",
    "no-empty": "warn",
    "no-unsafe-finally": "warn",
    "no-unsafe-negation": "warn",
    "no-prototype-builtins": "warn",
    "no-extra-boolean-cast": "warn",
    "no-extra-semi": "warn",
    "no-extra-parens": "warn",
    "no-extra-bind": "warn",
    "no-extra-label": "warn"
  }
}
